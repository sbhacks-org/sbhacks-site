---
title: SB Hacks VII
---

{% extends "layouts/base.njk" %}

{% block body %}
<div id="message">
{% block message %}
{% endblock %}
</div>
<div id = "landing">
  <div id="navBar">
    <ul>
      <li><a href="https://sbhacksvi.herokuapp.com/">SB HACKS VI</a></li>
    </ul>
  </div>

  <div class="fishSchool">
    <img src="/webAssets/Asset 17.svg" id="fishGroup">
    <img src="/webAssets/Asset 24.svg" id="fish2" class="fish">
    <img src="/webAssets/Asset 25.svg" id="fish3" class="fish">
    <img src="/webAssets/Asset 23.svg" id="fish1" class="fish">
  </div>

  <div id="center">
    <div id="waves">
      <img src="/webAssets/Asset 18.svg" id="waveGroup"> 
      <img src="/webAssets/Asset 9.svg" id="wave4" class="waves">
      <img src="/webAssets/Asset 8.svg" id="wave3" class="waves">
      <img src="/webAssets/Asset 7.svg" id="wave2" class="waves">
      <img src="/webAssets/Asset 10.svg" id="wave1" class="waves">
      <img src="/webAssets/Asset 21.svg" id="lights2" class="lights">
      <img src="/webAssets/Asset 20.svg" id="lights1" class="lights">
    </div>
    <img src="/webAssets/Asset 19.svg" id="spotLight" class="lights">
  </div>
      
  <div id="left">
    <img src="/webAssets/Asset 1.svg" id="leftBiome">
    <img src="/webAssets/Asset 29.svg" id="darkKelp">
    <img src="/webAssets/Asset 32.svg" id="leftLand">
    <img src="/webAssets/Asset 26.svg" id="greenKelp">
    <img src="/webAssets/Asset 27.svg" id="blueKelp">
  </div>

  <div id="right">
    <img src="/webAssets/Asset 2.svg" id="rightBiome">
  </div>
      
  <img src="/webAssets/Asset 42.svg" id="otter">
  
  <div id="centerInfo">
    <img src="/webAssets/Asset 22.svg" id="centerLogo">
    <div class="info-container">
      <h2 class="info info-item">January 15-17, 2021 | Online</h2>
    </div>
    <form id="mailinglist" action="/api/mailinglist" method="post">
      <div class="info-container">
        <h2 class="mailing-text">Subscribe to our mailing list for updates!</h2>
        <div class="aligned">
          <label for="email" class="visually-hidden">Email</label>
          <input type="email" id="email" placeholder="youremail@example.com" required name="email">
          <button type="submit" aria-label="Submit">
            <img src="/webAssets/Asset 14.svg" alt="Submit">
          </button>
        </div>
      </div>
    </form>
  </div>
</div>
{% endblock %}